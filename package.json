  "angular-cli": {},
  "scripts": {
    "start": "tsc -p ./src/client && concurrently \"npm run tsc:w:p ./src/client\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "typings": "typings",
    "develop": "concurrently \"tsc -w -p ./src/server\" \"tsc -w -p ./src/client\" \"nodemon ./src/server/bin/www\" ",
    "bundle": "node tools/builder.js",
    "bundle:prod": "node tools/builder.js --prod",
    "twc:client:w": "tsc -w -p ./src/client",
    "twc:server:w": "tsc -w -p ./src/server"
  },
  "private": true,
